{
  "name": "@cloudbase/mysql",
  "version": "2.25.1",
  "description": "cloudbase js sdk mysql module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "tsc": "rm -rf dist/ && tsc -p tsconfig.esm.json && tsc -p tsconfig.json",
    "tsc:watch": "tsc -p tsconfig.esm.json --watch & tsc -p tsconfig.json --watch",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "build": "npm run lint && npm run tsc",
    "dev": "npm run tsc:watch",
    "precommit": "npm run lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TencentCloudBase/cloudbase-js-sdk"
  },
  "keywords": [
    "tcb",
    "cloudbase",
    "javascript",
    "typescript",
    "mysql",
    "serverless"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@cloudbase/types": "2.20.3",
    "@cloudbase/utilities": "2.20.3",
    "@cloudbase/wx-cloud-client-sdk": "^1.7.1"
  },
  "devDependencies": {
    "typescript": "^4.9.5"
  },
  "gitHead": "6bd91c071d8fd06ef6d6d3ed2c8afcb84db4d9d5"
}
